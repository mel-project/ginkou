{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\",\"types\":[\"mocha\"]}' ./node_modules/.bin/mocha -r ts-node/register ./tests/*.spec.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@smui/banner": "^4.2.0",
    "@smui/data-table": "^4.2.0",
    "@smui/tab": "^4.2.0",
    "@smui/tab-bar": "^4.2.0",
    "@smui/top-app-bar": "^4.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "cross-fetch": "^3.1.4",
    "mocha": "^8.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.5.4",
    "svelte-preprocess": "^4.7.3",
    "ts-node": "^10.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-is": "^0.18.2"
  },
  "dependencies": {
    "@smui/button": "^4.2.0",
    "@smui/chips": "^4.2.0",
    "@smui/dialog": "^4.2.0",
    "@smui/icon-button": "^4.2.0",
    "@smui/layout-grid": "^4.2.0",
    "@smui/list": "^4.2.0",
    "@smui/menu": "^4.2.0",
    "@smui/select": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@types/json-bigint": "^1.0.0",
    "add": "^2.0.6",
    "json-bigint": "^1.0.0",
    "purify-ts": "^0.16.3",
    "sirv-cli": "^1.0.0",
    "text-decoding": "^1.0.0"
  }
}
